{
  "name": "github-extractor",
  "version": "0.0.1",
  "description": "Github Extractor: List or download a whole repo or specific files and folders very quickly",
  "main": "./dist/source/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "type": "module",
  "scripts": {
    "start:dev": "ts-node",
    "bench": "vitest bench --run",
    "test": "vitest --bail=1 --run --coverage --exclude \"**/*online*\"",
    "test:single": "vitest --bail=1",
    "test:online": "vitest online --bail=1",
    "lint": "npx eslint source --ext .mts",
    "lint:strict": "npx eslint --config ./eslint-strict.json source --ext .mts",
    "build": "tsc --build --incremental",
    "covbadge": "node ./dist/scripts/coverage-badge.mjs",
    "build:clean": "rimraf ./dist && tsc --build",
    "prepublish": "npm run test && npm run build:clean && npm run covbadge"
  },
  "keywords": [
    "Github",
    "templates",
    "tar",
    "tarball",
    "gzip",
    "download",
    "extract",
    "extractor",
    "github-extractor",
    "github_extractor",
    "repo",
    "repository",
    "files",
    "folders",
    "list",
    "quick",
    "fast",
    "fastest",
    "quickly",
    "specific",
    "whole",
    "git",
    "clone",
    "depth",
    "shallow"
  ],
  "author": "bn-l",
  "license": "ISC",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.3",
    "@swc/core": "^1.3.101",
    "@types/core-js": "^2.5.8",
    "@types/fs-minipass": "^2.1.4",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.3",
    "@types/tar": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "badgen": "^3.2.3",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "np": "^10.0.1",
    "rimraf": "^5.0.5",
    "simple-git": "^3.23.0",
    "sinon": "^17.0.1",
    "tinybench": "^2.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.1.0",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fastest-levenshtein": "^1.0.16",
    "tar": "^6.2.1",
    "undici": "^6.7.1"
  }
}
